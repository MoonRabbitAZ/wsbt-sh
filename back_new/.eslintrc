{
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "browser": true,
      "es2021": true,
      "commonjs": true
    },
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "prettier/prettier": "error",
      "printWidth": 120,
      "no-plusplus": 0,
      "no-await-in-loop": 0,
      "no-continue": 0,
      "no-restricted-syntax": 0,
      "no-param-reassign": 0,
      "guard-for-in": 0,
      "global-require": 0,
      "no-return-await": 0
    }
  }